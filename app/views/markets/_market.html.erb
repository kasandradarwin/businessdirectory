<div id="<%= dom_id market %>">
  <p>
    <strong>Title:</strong>
    <%= market.title %>
  </p>

  <p>
    <strong>Description:</strong>
    <%= market.description %>
  </p>

  <p>
    <strong>Address:</strong>
    <%= market.address %>
